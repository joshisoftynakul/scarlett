<%= form_for ([@podcast, @episode]) do |f| %>
	<%= f.label :title %><br>
	<%= f.text_field :title %>

	<br>
	<br>

	<%= f.label :rating %>
	<%= f.text_field :rating %>

	<br>
	<br>

	<%= f.label :episode_thumbnail %><br>
	<%= f.file_field :episode_thumbnail %>

	<%= f.submit class: "button" %>
<% end %>